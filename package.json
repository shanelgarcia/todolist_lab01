{
  "name": "todolistapp",
  "version": "1.0.0",
  "description": "Simple Vanilla JS App",
  "default": "src/index.html",
  "scripts": {
    "test": "none",
    "start": "npm run static && npm run dev",
    "static": "mkdir dist && cp -R src/static/data dist/data",
    "dev": "npx parcel src/index.html",
    "build": "npx parcel build src/index.html",
    "clean:dev": "del -f .parcel-cache,dist",
    "clean:git": "del -f .parcel-cache dist node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shanelgarcia/todolist_lab01.git"
  },
  "keywords": [
    "to",
    "do",
    "list"
  ],
  "author": "Shanel Garcia",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shanelgarcia/todolist_lab01/issues"
  },
  "homepage": "https://github.com/shanelgarcia/todolist_lab01#readme",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.0.0",
    "gulp": "^4.0.2",
    "http-server": "^13.0.2",
    "parcel": "^2.0.0",
    "sass": "^1.43.2"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "uuid": "^8.3.2"
  }
}
